Ex1,2
#include "Timer.h"
#include <iostream>
#include <fstream>
using namespace std;
int main(int argc, char* argv[]) {
	char* filename = argv[1];
	char* filename2 = argv[2];
	char* filename3 = argv[3];
	char* filename4 = argv[4];
	double start_time, finish_time;
	int max;
	int n;
	ifstream file;
	file.open(filename);
	file >> n;
	file >> max;
	int* numbers = new int[n];
	GET_TIME(start_time)
	for (int i = 0; i <= n; i++) {
		file >> numbers[i];
	}

	
	for (int i = 0; i <= n; i++) {
		if (numbers[i] > max)
			max = numbers[i];
	}
	GET_TIME(finish_time)
	cout << max << endl;
	cout << finish_time - start_time;;
}

