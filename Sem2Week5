Ex 1
//Iterative
#include <iostream>
using namespace std;
int main() {
	int n = 7;
	
	int f1 = 0;
	int f2 = 1;
	int sum = f1+f2;
	for (int i = 2; i < n; i++) {
		f1 = f2 + sum;
		f2 = sum;
		sum = f1;
	}
	cout << sum << endl;
}
-------------------------------------------------------------------------------------------------------------------
//Recursive
#include <iostream>
using namespace std;

int recursive(int n) {
	if (n == 0 || n==1) 
		return n;
	else 
		return recursive(n - 1) + recursive (n-2);
}
int main() {
	int n = 7;
	cout << recursive(n) << endl;
	
}
